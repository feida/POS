{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "shopDetail.json",
    "logs/*"
  ],
  "verbose": false,
  "execMap": {
    "": "node",
    "js": "node --harmony"
  },
  "events": {
    "restart": "javascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "ext": "js json",
  "legacy-watch": false
}